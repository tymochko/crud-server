<div class="table__wrapper">
    <table class="table">
        <tr class="table__row table__row--header">
            <th class="table__cell table__header">User name</th>
            <th class="table__cell table__header">Surname</th>
            <th class="table__cell table__header">Age</th>
            <th class="table__cell table__header">Action</th>
        </tr>
        {{#each table__row}}
        <tr class="table__row">
            <form class="table__form" method="get" action="/">
                <td class="table__cell">{{name}}</td>
                <td class="table__cell">{{surname}}</td>
                <td class="table__cell">{{age}}</td>
                <td class="table__cell">
                    <a href="/users/edit-user/3">Edit</a>
                    <a href="/users/delete/:3">Delete</a>
                </td>
            </form>
        </tr>
        {{/each}}
    </table>
</div>